{
  "framework": "nextjs",
  "functions": {
    "api/*.py": {
      "runtime": "vercel-python@0.1.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/backend.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 8 * * *"
    }
  ]
}